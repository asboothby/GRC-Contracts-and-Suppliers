<?xml version="1.0" encoding="UTF-8"?><record_update table="sys_ui_page">
    <sys_ui_page action="INSERT_OR_UPDATE">
        <category>general</category>
        <client_script><![CDATA[function invokePromptCallBack(type) {
var gdw = GlideDialogWindow.get();
if (type == 'ok')
var f = gdw.getPreference('onPromptContinue');
else
var f = gdw.getPreference('onPromptCancel');
if (typeof(f) == 'function') {
try {
f.call(gdw, gdw.getPreference('oldValue'));
} catch(e) {
}
}
gdw.destroy();
return false;
}
]]></client_script>
        <description/>
        <direct>false</direct>
        <endpoint>x_631631_grc_contr_confirm_ssp.do</endpoint>
        <html><![CDATA[<g:ui_form onsubmit="return invokeConfirmCallBack('ok');">
	<table width="100%">
		<tr><td/><td/></tr>
		<g:evaluate var='jvar_title' jelly='true' expression='RP.getWindowProperties().title' />
		<tr>
			<td nowrap="true"><g:no_escape>${jvar_title}</g:no_escape></td>
		</tr>
		<tr><td/><td/></tr>
		<tr>
			<td colspan="2" align="right">
				<g:dialog_buttons_ok_cancel
				ok_text="${gs.getMessage('Accept')}"
				ok="invokePromptCallBack('ok');"
				ok_type="button"
				cancel_text="${gs.getMessage('Cancel')}"
				cancel="invokePromptCallBack('cancel')"
				cancel_type="button" />
			</td>
		</tr>
	</table>
</g:ui_form>]]></html>
