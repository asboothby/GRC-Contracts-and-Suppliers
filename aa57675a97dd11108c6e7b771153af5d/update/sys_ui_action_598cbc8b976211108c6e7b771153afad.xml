        <script><![CDATA[var assmtQuestion = new GlideRecord('asmt_assessment_instance_question');
assmtQuestion.addQuery('source_id',current.supplier);
assmtQuestion.query();
while(assmtQuestion.next())
{
	var assmtID = assmtQuestion.instance;
	var assmt_inst = new GlideRecord('asmt_assessment_instance');
	assmt_inst.addQuery('sys_id', assmtID);
	assmt_inst.addQuery('state', 'complete');
	assmt_inst.query();

	while(assmt_inst.next())
	{
		assmt_inst.state='wip';
		assmt_inst.update();
	}
}


